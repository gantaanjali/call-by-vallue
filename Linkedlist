
#include <iostream>
#include <stdio.h>
using namespace std;
struct node{
    int data;
    struct node *next;
}*first=NULL;
void createnode(int A[], int n){
    struct node *t, *last;
    first = new node;
    first->data=A[0];
    first->next=NULL;
    last=first;
    for(int i=1;i<n;i++){
        t=new node;
        t->data=A[i];
        t->next=NULL;
        last->next=t;
        last =t;
    }
}
void Display(struct node *p){
    while(p!=NULL){
        cout<<p->data<<endl;
        p=p->next;
    }
}
int main()
{
   int a[]={1,2,3,4,5};
   createnode(a,5);
   Display(first);
    
}
